  "author": "Evangelos Pappas <epappas@evalonlabs.com>",
  "description": "",
  "main": "server",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:html": "copyfiles -f ./index.html ./dist",
    "copy:styles": "copyfiles -f ./styles/* ./dist/styles",
    "copy:statics": "copyfiles -f ./statics/* ./dist",
    "copy": "npm run copy:styles && npm run copy:statics",
    "build": "npm run clean && npm run copy",
    "serve": "NODE_ENV=production node server --harmony --env=production --progress --inline --colors",
    "serve-dev": "NODE_ENV=development node dev --harmony --env=development --progress --inline --colors",
    "start": "npm run build && npm run serve",
    "dev": "npm run build && npm run copy:html && npm run serve-dev",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "test": "NODE_ENV=test npm run lint & karma start",
    "test:watch": "NODE_ENV=test npm run lint & karma start --autoWatch=true --singleRun=false",
    "build:content": "node --harmony --progress --colors content/build.js"
  },
